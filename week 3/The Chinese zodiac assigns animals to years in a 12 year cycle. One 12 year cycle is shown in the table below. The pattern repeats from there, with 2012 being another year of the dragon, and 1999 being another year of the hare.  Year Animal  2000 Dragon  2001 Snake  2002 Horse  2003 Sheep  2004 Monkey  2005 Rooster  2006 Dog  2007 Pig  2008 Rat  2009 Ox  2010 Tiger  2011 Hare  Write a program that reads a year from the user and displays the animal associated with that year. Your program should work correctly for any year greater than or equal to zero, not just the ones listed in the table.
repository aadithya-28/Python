n=int(input())
temp=n
if n<2000:
    while(n<2000):
        n+=12
else:
    while(n>=2012):
        n-=12

        
if n==2000:
    print("%d is the year of the Dragon."%temp)
elif n==2001:
    print("%d is the year of the Snake."%temp)
elif n==2002:
    print("%d is the year of the Horse."%temp)
elif n==2003:
    print("%d is the year of the Sheep."%temp)
elif n==2004:
    print("%d is the year of the Monkey."%temp)
elif n==2005:
    print("%d is the year of the Rooster."%temp)
elif n==2006:
    print("%d is the year of the Dog."%temp)
elif n==2007:
    print("%d is the year of the Pig."%temp)
elif n==2008:
    print("%d is the year of the Rat."%temp)
elif n==2009:
    print("%d is the year of the Ox."%temp)
elif n==2010:
    print("%d is the year of the Tiger."%temp)
else:
    print("%d is the year of the Hare."%temp)
