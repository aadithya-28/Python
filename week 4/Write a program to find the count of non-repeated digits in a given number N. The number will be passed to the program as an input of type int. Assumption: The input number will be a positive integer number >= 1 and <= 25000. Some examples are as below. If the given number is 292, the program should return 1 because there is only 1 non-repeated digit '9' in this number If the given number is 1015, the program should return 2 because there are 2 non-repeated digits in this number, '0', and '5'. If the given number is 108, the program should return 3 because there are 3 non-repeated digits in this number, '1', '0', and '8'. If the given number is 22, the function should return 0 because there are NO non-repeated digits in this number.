#non repeated digit
n=int(input())
s=int(input())
l=list(set(s))
d=0
for i in range(0,len(l)):
             c=0
             for j in range(0,len(s)):
                               if int(l[i])==int(s[j]):
                                                c=c+1
             if c==1:
                      d=d+1
print(d)
